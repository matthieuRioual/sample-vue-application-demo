{
  "name": "jhipster",
  "devDependencies": {
    "sinon": "14.0.0",
    "@types/sinon": "10.0.11",
    "vue-tsc": "0.37.7",
    "vue-jest": "5.0.0-alpha.10",
    "vite": "2.9.12",
    "typescript": "4.7.3",
    "ts-jest": "26.5.6",
    "jest-transform-stub": "2.0.0",
    "jest-sonar-reporter": "2.0.0",
    "jest": "26.6.3",
    "eslint-plugin-vue": "8.7.1",
    "eslint-config-prettier": "8.5.0",
    "eslint": "8.17.0",
    "@vue/test-utils": "2.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vitejs/plugin-vue": "2.3.3",
    "@typescript-eslint/parser": "5.28.0",
    "@types/jest": "28.1.1",
    "@rushstack/eslint-patch": "1.1.3",
    "prettier-plugin-packagejson": "2.2.18",
    "prettier-plugin-java": "1.6.2",
    "prettier": "2.6.2",
    "lint-staged": "13.0.1",
    "husky": "8.0.1",
    "@prettier/plugin-xml": "2.2.0"
  },
  "scripts": {
    "test:watch": "npm run jest -- --watch",
    "test": "npm run jest --",
    "preview": "vite preview",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "dev": "vite",
    "jest": "jest src/test/javascript/spec --logHeapUsage --maxWorkers=2 --no-cache",
    "start": "vite",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue-router": "4.0.16",
    "axios": "0.27.2",
    "vue": "3.2.37"
  },
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Project",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  },
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
